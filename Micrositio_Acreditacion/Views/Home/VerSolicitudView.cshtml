                <table id="tablesol" class="table table-striped table-responsive-lg" align="center" cellpadding="2" style="overflow-y: scroll; overflow-x: scroll;">

                    <thead class="clearfix text-center">

                        <tr>
                            <th>Cuit </th>
                            <th>Cuenta </th>
                            <th>Fecha de Subida </th>
                            <th>Nombre de Archivo </th>
                            <th>Detalle</th>





                        </tr>

                    </thead>
                    <tbody style="overflow-x: scroll;  " class="text-center">

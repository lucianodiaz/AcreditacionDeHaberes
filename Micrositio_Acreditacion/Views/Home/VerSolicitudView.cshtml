                                <td>
                                    @m.cuenta
                                </td>
                                <td>@m.fecha</td>

                                <td>@m.nombreArchivo</td>
                                <td>
                                    <form action="@Url.Action("VerDetalleSolicitudView","home")" method="post">
                                        @*toma cuit empresa y el id del documento*@
                                        <input type="text" name="cuitEmpresa" value="@m.cuit" hidden />
                                        <input type="text" name="idCarga" value="@m.id" hidden />
                                        <button type="submit" class="btn btn-success "><i class="fas fa-file-download"></i></button>
                                    </form>
                                </td>

                            </tr>

                        }





